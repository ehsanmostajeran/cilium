{
	"kind": "Pod",
	"apiVersion": "v1",
	"metadata": {
		"name": "redis-controller-tester",
		"labels": {
			"app": "redis-controller",
			"com.docker.compose.service": "redis-controller"
		}
	},
	"spec": {
		"containers": [
		{
			"name": "redis",
			"image": "redis",
			"ports": [
			{
				"containerPort": 6379,
				"protocol": "TCP"
			}
			]
		}
		]
	}
}
